{
  "buildCommand": "node scripts/build-web.cjs",
  "outputDirectory": "dist",
  "devCommand": "npx expo start --web",
  "framework": null,
  "rewrites": [
    { "source": "/api/(.*)", "destination": "/api/$1" },
  { "source": "/web", "destination": "/web/index.html" },
  { "source": "/web/(.*)", "destination": "/web/index.html" },
  { "source": "/_expo/(.*)", "destination": "/web/_expo/$1" },
  { "source": "/favicon.ico", "destination": "/web/favicon.ico" },
    { "source": "/", "destination": "/landing/index.html" }
  ]
}
